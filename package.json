{
  "name": "rebrowser",
  "author": {
    "name": "Nicolas Lopez"
  },
  "version": "2.1.3",
  "private": true,
  "homepage": ".",
  "main": "./build/electron/main.js",
  "repository": "https://github.com/nicolaslopezj/rebrowser",
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "axios": "^1.5.0",
    "classnames": "^2.3.2",
    "dot-object": "^2.1.4",
    "easy-auto-launch": "^6.0.2",
    "electron-builder-notarize": "^1.5.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^5.0.0-beta.28",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-store": "^8.1.0",
    "electron-updater": "6.1.8",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-app-events": "^2.0.2",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "react-scripts": "5.0.1",
    "simple-react-form": "^4.0.6",
    "typescript": "^5.4.5",
    "web-vitals": "^3.4.0"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://127.0.0.1:5149 && tsc -p electron -w\" \"wait-on http://127.0.0.1:5149 && tsc -p electron && electron .\"",
    "compile": "rm -rf ./dist && yarn build && tsc -p electron",
    "release": "electron-builder --config .electron-builder.config.js --win --mac",
    "start": "PORT=5149 react-scripts start",
    "build-win": "electron-builder --config .electron-builder.config.js --win",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": ["react-app", "react-app/jest"]
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": ["last 1 chrome version", "last 1 firefox version", "last 1 safari version"]
  },
  "devDependencies": {
    "@electron/notarize": "^2.3.2",
    "@types/dot-object": "^2.1.6",
    "@types/electron": "^1.6.10",
    "@types/electron-devtools-installer": "^2.2.5",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.5",
    "@types/node": "20.14.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "cli-truncate": "2.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dmg-license": "^1.0.11",
    "electron": "^31.2.1",
    "electron-builder": "^24.13.3",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.6",
    "wait-on": "^7.2.0"
  }
}
